package text;

public class Max {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
     maxnum ma=new maxnum();
     min mi=new min();
     System.out.println(ma.maxnum(30, 44.5));
    System.out.println(mi.minnum(12, 10));
	}

}
//选出两个参数的最大值
class maxnum{
	public double maxnum(double x,double y) {
		if (x>y) {
			return x;
		}
		else {
			return y;
		}
		}
	}
class min{
	public double minnum(double x,double y) {
		if (x>y) {
			return x;
		}
		else {
			return y;
		}
		}
}
}
