package text;

public class Stringtest {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
   //  System.out.println("cc");
		str st=new str();
		System.out.println(st.str("hellojava")+"int类型"+st.Int(100));
		
	}
	

}
//创建一个类  实例化类对象
class str{
	//返回值 有参数 返回参数
	public String str(String a) {
		return a;
		
	}
	public int Int(int b) {
		
		return b;
	}
}
