package text;
import java.awt.*;
import java.util.Scanner;




public class Test2 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Scanner sc=new Scanner(System.in);
		//scanner 方法允许输入
		String name;
		
		System.out.println("输入名字");
		name=sc.nextLine();
		System.out.println("你输入的名字是："+name);
	}

}
